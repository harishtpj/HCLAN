# Program to Input value from user and output basic arith operations

let a := num(input("Enter no 1: "));
let b := num(input("Enter no 2: "));

println fmt("%d + %d = %d", a, b, a + b);
println fmt("%d - %d = %d", a, b, a - b);
println fmt("%d * %d = %d", a, b, a * b);
println fmt("%d / %d = %s", a, b, a / b);